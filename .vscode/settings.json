{
  "editor.fontFamily": "JetBrains Mono, Monaspace Argon, Geist Mono, Dank Mono, Fira Code, Operator Mono Lig, Hack",
  "editor.fontSize": 18,
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 200,
  "editor.minimap.showSlider": "always",
  "editor.minimap.enabled": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 26,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
  },
  "editor.semanticHighlighting.enabled": false,
  "editor.autoClosingBrackets": "always",
  "editor.linkedEditing": true,
  "editor.inlineSuggest.enabled": true,
  "editor.rulers": [
      80
  ],
  "editor.stickyScroll.enabled": true,
  "editor.wordBasedSuggestions": "off",
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "editor.bracketPairColorization.enabled": true,
  "explorer.openEditors.visible": 1,
  "telemetry.telemetryLevel": "off",
  "telemetry.telemetryEnabled": false,
  "workbench.activityBar.orientation": "vertical",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.colorCustomizations": {
      "terminal.foreground": "#eff0eb",
      "terminal.ansiBlack": "#282a36",
      "terminal.ansiRed": "#ff5c57",
      "terminal.ansiGreen": "#5af78e",
      "terminal.ansiYellow": "#f3f99d",
      "terminal.ansiBlue": "#57c7ff",
      "terminal.ansiMagenta": "#ff6ac1",
      "terminal.ansiCyan": "#9aedfe",
      "terminal.ansiWhite": "#f1f1f0",
      "terminal.ansiBrightBlack": "#686868",
      "terminal.ansiBrightRed": "#ff5c57",
      "terminal.ansiBrightGreen": "#5af78e",
      "terminal.ansiBrightYellow": "#f3f99d",
      "terminal.ansiBrightBlue": "#57c7ff",
      "terminal.ansiBrightMagenta": "#ff6ac1",
      "terminal.ansiBrightCyan": "#9aedfe",
      "terminal.ansiBrightWhite": "#eff0eb"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "window.clickThroughInactive": true,
  "explorer.confirmDragAndDrop": false,
  "breadcrumbs.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
      "USE_GITIGNORE": true,
      ".next": true,
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.history": true,
      "**/node_modules": true,
      "jspm_packages": true,
      "node_modules": true
  },
  "files.watcherExclude": {
      "/.next/": true,
      "/node_modules/": true
  },
  "search.exclude": {
      "/.next": true,
      "/node_modules": true,
      "**/node_modules": true,
      "**/bower_components": true,
      "**/coverage": true,
      "**/dist": true,
      "**/build": true,
      "**/.build": true,
      "**/.gh-pages": true
  },
  "emmet.includeLanguages": {
      "javascript": "javascriptreact",
      "typescriptreact": "javascriptreact",
      "reason": "javascriptreact",
      "postcss": "css"
  },
  "emmet.syntaxProfiles": {
      "javascript": "jsx",
      "typescriptreact": "jsx"
  },
  "emmet.triggerExpansionOnTab": false,
  "postcss.validate": false,
  "eslint.validate": [
      "vue",
      "svelte",
      "astro",
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
  ],
  "eslint.workingDirectories": [
      "./src",
      "./app"
  ],
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "gitlens.advanced.messages": {
      "suppressCommitHasNoPreviousCommitWarning": false,
      "suppressCommitNotFoundWarning": false,
      "suppressFileNotUnderSourceControlWarning": false,
      "suppressGitVersionWarning": false,
      "suppressLineUncommittedWarning": false,
      "suppressNoRepositoryWarning": false
  },
  "gitlens.keymap": "chorded",
  "gitlens.hovers.currentLine.over": "line",
  "git.confirmSync": false,
  "prettier.documentSelectors": [
      "**/*.astro"
  ],
  "[astro]": {
      "editor.defaultFormatter": "astro-build.astro-vscode",
      "editor.formatOnSave": true
  },
  "[elm]": {
      "editor.formatOnSave": true
  },
  "[svelte]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "svelte.enable-ts-plugin": true,
  "[dart]": {
      "editor.formatOnSave": true
  },
  "[typescriptreact]": {
      "editor.autoClosingBrackets": "always",
      "editor.suggestSelection": "recentlyUsed",
      "editor.suggest.showKeywords": false,
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      // "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
      "editor.autoClosingBrackets": "always",
      "editor.suggestSelection": "recentlyUsed",
      "editor.suggest.showKeywords": false,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
      // "editor.defaultFormatter": "biomejs.biome",
  },
  "[javascript]": {
      "editor.autoClosingBrackets": "always",
      "editor.suggestSelection": "recentlyUsed",
      "editor.suggest.showKeywords": false,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
      "editor.defaultFormatter": "vscode.html-language-features"
  },
  "liveshare.featureSet": "insiders",
  "typescript.tsserver.maxTsServerMemory": 8192,
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Ayu Mirage Bordered",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "github.copilot.enable": {
      "*": false,
      "plaintext": true,
      "markdown": true,
      "scminput": false,
      "yaml": false,
      "typescript": true,
      "typescriptreact": true
  },
  "[json]": {
      // "editor.defaultFormatter": "biomejs.biome"
      "editor.defaultFormatter": "vscode.json-language-features"
  },
  "liveshare.presence": true,
  "docwriter.hotkey.windows": "Alt + .",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": true,
  "svelte.plugin.svelte.note-new-transformation": false,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "docwriter.hotkey.mac": "‚å• + .",
  "vsicons.dontShowNewVersionMessage": true,
  "tailwindCSS.experimental.classRegex": [
      [
          "cva\\(([^)]*)\\)",
          "[\"'`]([^\"'`]*).*?[\"'`]"
      ]
  ],
  "importCost.debug": true,
  "importCost.timeout": 100000,
  "importCost.typescriptExtensions": [
      "\\.ts(x)?$"
  ],
  "rapidapi.terminalLink.enabled": false,
  "react-native-tools.showUserTips": false,
  "git.ignoreRebaseWarning": true,
  "[css]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "errorLens.excludeBySource": [
      "eslint(@typescript-eslint/no-non-null-assertion)",
      "eslint(@typescript-eslint/no-unsafe-member-access)",
      "eslint(@typescript-eslint/no-unnecessary-condition)"
  ],
  "git.openRepositoryInParentFolders": "never",
  "githubPullRequests.createOnPublishBranch": "never",
  "window.commandCenter": false,
  "githubPullRequests.pullBranch": "never",
  "terminal.integrated.env.linux": {},
  "console-ninja.featureSet": "Community",
  "terminal.integrated.env.windows": {},
  "editor.cursorBlinking": "phase",
  "terminal.integrated.env.osx": {},
  "workbench.tree.enableStickyScroll": true,
  "remote.tunnels.access.hostNameOverride": "goncy",
  "console-ninja.warnOnNoToolsDetected": false,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.port": 3000,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "explorer.fileNesting.patterns": {
      "*.tsx": "${capture}.tsx, ${capture}.client.*, ${capture}.styles.*, ${capture}.stories.*, ${capture}.test.*"
  },
  "githubPullRequests.pushBranch": "always",
  "prisma.showPrismaDataPlatformNotification": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "playwright.reuseBrowser": false,
  "playwright.showTrace": true,
  "supermaven.enable": {
      "*": false
  },
  "cursor.cpp.disabledLanguages": [
      "scminput",
      "yaml"
  ],
  "workbench.sideBar.location": "right",
  "remote.SSH.remotePlatform": {
      "157.245.128.116": "linux"
  },
  "vue.server.hybridMode": false,
  "tailwind-fold.autoFold": false,
}